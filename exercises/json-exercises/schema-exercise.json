{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "exercise_name": { "type": "string" },
        "exercise_description": { "type": "string" },
        "reps": { "type": "integer" },
        "reps_range": { "type": "string" },
        "sets": { "type": "integer" },
        "sets_range": { "type": "string" },
        "time_exercise": { "type": "integer" },
        "time_exercise_range": { "type": "string" },
        "time_pause": { "type": "integer" },
        "time_pause_set": { "type": "integer" },
        "type": { "type": "string" },
        "sub_type": { "type": "string" },
        "level_text": { "type": "string" },
        "reference": { "type": "string" },
        "test":{
            "type": "array",
            "items": {
                "type": "string",
                "enum": ["one", "two", "three"]
            }   
        }
    },
    "additionalProperties": false,
    "required": ["exercise_name"]
}
