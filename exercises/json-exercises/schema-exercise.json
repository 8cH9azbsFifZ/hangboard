{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "exercise_name": { "type": "string" },
        "exercise_description": { "type": "string" },
        "reps": { "type": "integer" },
        "reps_range": { "type": "string" },
        "sets": { "type": "integer" },
        "sets_range": { "type": "string" },
        "time_exercise": { "type": "integer" },
        "time_exercise_range": { "type": "string" },
        "time_pause": { "type": "integer" },
        "time_pause_set": { "type": "integer" },
        "type": { "type": "string" },
        "sub_type": { "type": "string" },
        "level_text": { "type": "string" },
        "focus": { "type": "string"}, 
        "left_hold": { "type": "string"}, 
        "right_hold": { "type": "string"}, 
        "reference": { "type": "string" },
        "needs_rings": { "type": "boolean"},
        "needs_pullupbar": { "type": "boolean"},
        "needs_mat": { "type": "boolean"},
        "needs_weights": { "type": "boolean"},
        "needs_climbingwall": { "type": "boolean"},
        "route_grades_range": { 
            "type": "array", 
            "items": {
                "type": "string"
            }
        },
        "url_video": { "type": "string"},
        "url_image": { "type": "string"},
        "grip":{
            "type": "string",
            "items": {
                "type": "string",
                "enum": ["4 Fingers", "4 Fingers half crimp", "4 Fingers crimped ", "4 Fingers chisel", "open 3", "front 3 half crimp", "back 3 fingers half crimp", "front 2", "middle 2", "back 2", "middle finger mono"]
            }   
        }
    },
    "additionalProperties": false,
    "required": ["exercise_name"]
}
